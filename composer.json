        "ext-apcu": "*",
        "ext-pdo": "*",
        "ext-iconv": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "aferrandini/phpqrcode": "^1.0",
        "ezyang/htmlpurifier": "^4.13",
        "kint-php/kint": "^3.3",
